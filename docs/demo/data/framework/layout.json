{
  "tag": "layout-fit",
  "className": "padding10 h100 overflow-auto",
  "initStore": {
    "apiList": [
      {
        "code": "tag",
        "name": "layout",
        "type": "string",
        "description": "页面级布局标签"
      }
    ]
  },
  "children": [
    {
      "tag": "h1",
      "className": "border-bottom",
      "children": "layout示例"
    },
    {
      "tag": "label",
      "className": "padding-left10 padding-bottom10",
      "children": "继承ant的layout"
    },
    {
      "tag": "row",
      "children": [
        {
          "tag": "col",
          "span": 12,
          "style": {
            "height": "400px",
            "overflow": "auto"
          },
          "children": [
            {
              "tag": "h3",
              "children": "代码配置示例",
              "className": "border-bottom"
            },
            {
              "tag": "coder",
              "language": "json",
              "value": {
                "tag": "layout",
                "children": [
                  {
                    "tag": "header",
                    "children": "Header"
                  },
                  {
                    "tag": "content",
                    "children": "Content"
                  },
                  {
                    "tag": "footer",
                    "children": "Footer"
                  }
                ]
              }
            }
          ]
        },
        {
          "tag": "col",
          "span": 12,
          "children": [
            {
              "tag": "h3",
              "children": "配置简述",
              "className": "border-bottom"
            },
            {
              "tag": "ol",
              "children": [
                {
                  "tag": "li",
                  "children": "请看ant 的layout 文档"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "tag": "h3",
      "className": "border-bottom",
      "children": "API（注意：configNode表示低代码配置，action表示动作配置）"
    },
    {
      "tag": "row",
      "children": [
        {
          "tag": "layout-fit",
          "style": {
            "height": "200px",
            "width": "100%"
          },
          "children": [
            {
              "tag": "table",
              "rowData": "${apiList}",
              "tooltipShowDelay": 0,
              "columnDefs": [
                {
                  "headerName": "#",
                  "width": 60,
                  "suppressSizeToFit": true,
                  "valueGetter": "node.rowIndex + 1",
                  "sortable": false,
                  "pinned": "left"
                },
                {
                  "headerName": "字段",
                  "field": "code",
                  "width": 150,
                  "suppressSizeToFit": true
                },
                {
                  "headerName": "名称",
                  "field": "name",
                  "width": 150,
                  "suppressSizeToFit": true
                },
                {
                  "headerName": "类型",
                  "field": "type",
                  "width": 150,
                  "suppressSizeToFit": true
                },
                {
                  "headerName": "描述",
                  "field": "description",
                  "tooltipField": "description"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "tag": "h3",
      "className": "border-bottom",
      "children": "展示示例"
