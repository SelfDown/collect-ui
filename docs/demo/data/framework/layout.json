            {
              "tag": "table",
              "rowData": "${apiList}",
              "tooltipShowDelay": 0,
              "columnDefs": [
                {
                  "headerName": "#",
                  "width": 60,
                  "suppressSizeToFit": true,
                  "valueGetter": "node.rowIndex + 1",
                  "sortable": false,
                  "pinned": "left"
                },
                {
                  "headerName": "字段",
                  "field": "code",
                  "width": 150,
                  "suppressSizeToFit": true
                },
                {
                  "headerName": "名称",
                  "field": "name",
                  "width": 150,
                  "suppressSizeToFit": true
                },
                {
                  "headerName": "类型",
                  "field": "type",
                  "width": 150,
                  "suppressSizeToFit": true
                },
                {
                  "headerName": "描述",
                  "field": "description",
                  "tooltipField": "description"
                }
              ]
            }
          ]
        }
      ]
