{
  "tag": "layout-fit",
  "className": "padding10 h100 overflow-auto",
  "initStore": {
    "apiList": [
      {
        "code": "tag",
        "name": "layout",
        "type": "string",
        "description": "页面级布局标签"
      },
      {
        "code": "tag",
        "name": "sider",
        "type": "string",
        "description": "侧边栏。必须包裹在layout下"
      },
      {
        "code": "tag",
        "name": "header",
        "type": "string",
        "description": "顶部。必须包裹在layout下"
      },
      {
        "code": "tag",
        "name": "content",
        "type": "string",
        "description": "内容。必须包裹在layout下"
      },
      {
        "code": "tag",
        "name": "footer",
        "type": "string",
        "description": "底部。必须包裹在layout下"
      }
    ]
  },
  "children": [
    {
      "tag": "h1",
      "className": "border-bottom",
      "children": "layout示例"
    },
    {
      "tag": "label",
      "className": "padding-left10 padding-bottom10",
      "children": [
        "继承ant的layout,改了部分样式比内容位置固定",
        {
          "tag": "a",
          "target": "_blank",
          "href": "https://ant.design/components/layout-cn",
          "children": "查看ant layout文档"
        }
      ]
    },
    {
      "tag": "row",
      "children": [
        {
          "tag": "col",
          "span": 12,
          "style": {
            "height": "400px",
            "overflow": "auto"
          },
          "children": [
            {
              "tag": "h3",
              "children": "代码配置示例",
              "className": "border-bottom"
            },
            {
              "tag": "coder",
              "language": "json",
              "value": {
                "tag": "layout",
                "children": [
                  {
                    "tag": "header",
                    "children": "Header"
                  },
                  {
                    "tag": "content",
                    "children": "Content"
                  },
                  {
                    "tag": "footer",
                    "children": "Footer"
                  }
                ]
              }
            }
          ]
        },
        {
          "tag": "col",
          "span": 12,
          "children": [
            {
              "tag": "h3",
              "children": "配置简述",
              "className": "border-bottom"
            },
            {
              "tag": "ol",
              "children": [
                {
                  "tag": "li",
                  "children": "header-头部"
                },
                {
                  "tag": "li",
                  "children": "content-内容"
                },
                {
                  "tag": "li",
                  "children": "footer-底部"
                },
                {
                  "tag": "li",
                  "children": "sider-侧边栏"
                },
                {
                  "tag": "li",
                  "children": [
