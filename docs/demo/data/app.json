{
  "tag": "router",
  "data_home": "/collect-ui/demo/data",
  "basename":"/collect-ui",
  "hash": true,
  "router": [
    {
       "path": "/",
       "redirect": "/framework/"
    },
    {
      "path": "/framework",
      "data": "framework.json",
      "children": [
        {
          "path": "/framework/",
          "data": "framework/home.json"
        },
        {
          "path": "/framework/install",
          "data": "framework/install.json"
        },
        {
          "path": "/framework/user",
          "data": "framework/user.json"
        },
        {
          "path": "/framework/role",
          "data": "framework/role.json"
        }
      ]
    }
  ]
}